{
    "name": "elmijo/php-html-dom",
    "description": "Esta Es una Herramienta que permite convertir y manipular una cadena de texto con formato html en un objeto php.",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": ">=5.4.0",
        "symfony/css-selector": "2.6.*",
        "elmijo/php-cliente-addr": "dev-master",
        "elmijo/php-error-log": "1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jerry Anselmi",
            "email": "jerry.anselmi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PHPTools\\PHPHtmlDom\\":"src/PHPHtmlDom/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "minimum-stability": "stable"
}
